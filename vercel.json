{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "bash build_files.sh",
        "runtime": "python3.9"
      }
    },
    {
      "src": "check.sh",
      "use": "@vercel/bash",
      "config": {
        "buildCommand": "bash check.sh"
      }
    }
   
    
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ]
}

